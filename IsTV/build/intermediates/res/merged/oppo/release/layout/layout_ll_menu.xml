    <TextView
        android:id="@+id/tv_my_shoucang"
        android:layout_width="fill_parent"
        android:layout_height="45dp"
        android:layout_marginTop="15dp"
        android:background="#2e2e2e"
        android:drawableLeft="@drawable/icon_shoucang_select"
        android:drawablePadding="17dp"
        android:drawableRight="@drawable/icon_inter_select"
        android:gravity="center_vertical"
        android:paddingLeft="17dp"
        android:paddingRight="17dp"
        android:text="我的收藏"
        android:textColor="@drawable/text_color_white_and_gray_selector" />

    <TextView
        android:id="@+id/tv_my_jilu"
        android:layout_width="fill_parent"
        android:layout_height="45dp"
        android:layout_marginTop="1.5dp"
        android:background="#2e2e2e"
        android:drawableLeft="@drawable/icon_jilu_select"
        android:drawablePadding="17dp"
        android:drawableRight="@drawable/icon_inter_select"
        android:gravity="center_vertical"
        android:paddingLeft="17dp"
        android:paddingRight="17dp"
        android:text="观看记录"
        android:textColor="@drawable/text_color_white_and_gray_selector" />

    <TextView
        android:id="@+id/tv_show_cost"
        android:layout_width="fill_parent"
        android:layout_height="45dp"
        android:layout_marginTop="15dp"
        android:background="#2e2e2e"
        android:drawableLeft="@drawable/icon_xiaofei_select"
        android:drawablePadding="17dp"
        android:drawableRight="@drawable/icon_inter_select"
        android:gravity="center_vertical"
        android:paddingLeft="17dp"
        android:paddingRight="17dp"
        android:text="消费记录"
        android:textColor="@drawable/text_color_white_and_gray_selector"
        android:visibility="gone" />

    <TextView
        android:id="@+id/tv_my_youhuiquan"
        android:layout_width="fill_parent"
        android:layout_height="45dp"
        android:layout_marginTop="1.5dp"
        android:background="#2e2e2e"
        android:drawableLeft="@drawable/icon_youhui_select"
        android:drawablePadding="17dp"
        android:drawableRight="@drawable/icon_inter_select"
        android:gravity="center_vertical"
        android:paddingLeft="17dp"
        android:paddingRight="17dp"
        android:text="优惠券"
        android:textColor="@drawable/text_color_white_and_gray_selector" />

    <TextView
        android:id="@+id/tv_login_qrcode"
        android:layout_width="fill_parent"
        android:layout_height="45dp"
        android:layout_marginTop="15dp"
        android:background="#2e2e2e"
        android:drawableLeft="@drawable/item_saoyisao_select"
        android:drawablePadding="17dp"
        android:drawableRight="@drawable/icon_inter_select"
        android:gravity="center_vertical"
        android:paddingLeft="17dp"
        android:paddingRight="17dp"
        android:text="二维码登录"
        android:textColor="@drawable/text_color_white_and_gray_selector" />

    <TextView
        android:id="@+id/tv_user_report"
        android:layout_width="fill_parent"
        android:layout_height="45dp"
        android:layout_marginTop="1.5dp"
        android:layout_marginBottom="15dp"
        android:background="#2e2e2e"
        android:drawableLeft="@drawable/icon_fankui_select"
        android:drawablePadding="17dp"
        android:drawableRight="@drawable/icon_inter_select"
        android:gravity="center_vertical"
        android:paddingLeft="17dp"
        android:paddingRight="17dp"
        android:text="用户反馈"
        android:textColor="@drawable/text_color_white_and_gray_selector" />
