<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/layout_myspace"
        android:layout_width="fill_parent"
        android:layout_height="45dp"
        android:layout_marginTop="15dp"
        android:background="#2e2e2e">

        <ImageView
            android:id="@+id/img_left_myspace"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="17dp"
            android:src="@drawable/icon_want_selector" />

        <TextView
            android:id="@+id/text_myspace"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="17dp"
            android:layout_toRightOf="@+id/img_left_myspace"
            android:text="我要直播"
            android:textColor="@drawable/text_color_white_and_gray_selector" />

        <ImageView
            android:id="@+id/new_myspace"
            android:layout_width="4dp"
            android:layout_height="4dp"
            android:layout_above="@+id/text_myspace"
            android:layout_toRightOf="@+id/text_myspace"
            android:src="@drawable/dot_normal_bluedot"
            android:visibility="visible" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="17dp"
            android:src="@drawable/icon_inter_select" />
    </RelativeLayout>

    <TextView
        android:id="@+id/tv_my_zhanneixin"
        android:layout_width="fill_parent"
        android:layout_height="45dp"
        android:layout_marginTop="1.5dp"
        android:background="#2e2e2e"
        android:drawableLeft="@drawable/icon_zhanneixin_select"
        android:drawablePadding="17dp"
        android:drawableRight="@drawable/icon_inter_select"
        android:gravity="center_vertical"
        android:paddingLeft="17dp"
        android:paddingRight="17dp"
        android:text="站内信"
        android:textColor="@drawable/text_color_white_and_gray_selector"
        android:visibility="gone" />

    <TextView
        android:id="@+id/tv_my_chongzhi"
        android:layout_width="fill_parent"
        android:layout_height="45dp"
        android:layout_marginTop="1.5dp"
        android:background="#2e2e2e"
        android:drawableLeft="@drawable/icon_chongzhi_selector"
        android:drawablePadding="17dp"
        android:drawableRight="@drawable/icon_inter_select"
        android:gravity="center_vertical"
        android:paddingLeft="17dp"
        android:paddingRight="17dp"
        android:text="充值"
        android:textColor="@drawable/text_color_white_and_gray_selector" />

    <TextView
        android:id="@+id/tv_my_rank"
        android:layout_width="fill_parent"
        android:layout_height="45dp"
        android:layout_marginTop="15dp"
        android:background="#2e2e2e"
        android:drawableLeft="@drawable/icon_paihang_select"
        android:drawablePadding="17dp"
        android:drawableRight="@drawable/icon_inter_select"
        android:gravity="center_vertical"
        android:paddingLeft="17dp"
        android:paddingRight="17dp"
        android:text="排行榜"
        android:textColor="@drawable/text_color_white_and_gray_selector" />

    <TextView
        android:id="@+id/tv_my_calendor"
        android:layout_width="fill_parent"
        android:layout_height="45dp"
        android:layout_marginTop="1.5dp"
        android:background="#2e2e2e"
        android:drawableLeft="@drawable/icon_calendor_select"
        android:drawablePadding="17dp"
        android:drawableRight="@drawable/icon_inter_select"
        android:gravity="center_vertical"
        android:paddingLeft="17dp"
        android:paddingRight="17dp"
        android:text="日程表"
        android:textColor="@drawable/text_color_white_and_gray_selector" />
    <!--
<TextView
        android:id="@+id/tv_my_order"
