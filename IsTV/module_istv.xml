      <bootclasspath refid="istv.module.bootclasspath"/>
      <classpath refid="istv.module.production.classpath"/>
      <src refid="istv.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.istv"/>
    </javac2>
    
    <copy todir="${istv.output.dir}">
